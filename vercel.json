{
  "version": 2,
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm run build",
  "functions": {
    "dist/index.js": {
      "runtime": "nodejs22.x",
      "includeFiles": "dist/public/**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}